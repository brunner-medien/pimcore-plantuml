{
  "name" : "brunner-medien/pimcore-plantuml",
  "type" : "pimcore-bundle",
  "license": "lgpl-3.0",
  "description" : "PlantUML class diagram generation based on pimcore class definitions",
  "keywords" : [
    "pimcore",
    "plantuml"
  ],
  "homepage" : "https://github.com/brunner-medien/pimcore-plantuml",
  "authors" : [{
    "name" : "Brunner Medien AG Michael Reufer",
    "email" : "m.reufer@bag.ch",
    "homepage" : "https://www.bag.ch/",
    "role" : "Developer"
  }
  ],
  "autoload": {
    "psr-4": {
      "PlantUmlBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "PlantUmlBundle\\PlantUmlBundle"
      ]
    }
  },
  "require": {
    "pimcore/pimcore": "^10.0",
    "jawira/plantuml-encoding": "^1.0"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^10.0",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-symfony": "^1.1"
  }
}